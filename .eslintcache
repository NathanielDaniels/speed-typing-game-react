Desktop\\Projects\\speed-typing-game-react\\src\\hooks\\useWordGame.js",["30"],"import {useState, useEffect, useRef}from 'react'\r\n\r\nfunction useWordGame(startTime = 15) {\r\n  const [text, setText] = useState(\"\")\r\n  const [timeRemaining, setTimeRemaining] = useState(startTime)\r\n  const [isTimeRunning, setIsTimeRunning] = useState(false)\r\n  const [wordCount, setWordCount] = useState('')\r\n  const textBoxRef = useRef(null)\r\n\r\n  function handleChange(e) {\r\n    const {value} = e.target\r\n    setText(value)\r\n  }\r\n\r\n  function calculateWordCount(text) {\r\n    const wordArray = text.trim().split(' ')\r\n    return wordArray.filter((word) => word !== '' && word !== ',' && word !== '.' && word !== '/' && word !== '?' && word !== '>' && word !== '<').length\r\n  }\r\n\r\n  function endGame() {\r\n    setIsTimeRunning(false)\r\n    setTimeRemaining(\"Times Up!\")\r\n    setWordCount(calculateWordCount(text))\r\n  }\r\n\r\n  function startGame() {\r\n    setIsTimeRunning(true)\r\n    setText(\"\")\r\n    setTimeRemaining(startTime)\r\n    setWordCount('-')\r\n    textBoxRef.current.disabled = false\r\n    textBoxRef.current.focus()\r\n  }\r\n\r\n  useEffect(() => {\r\n    if (isTimeRunning && timeRemaining > 0) {\r\n      setTimeout(() => {\r\n        setTimeRemaining(prevTime => prevTime - 1)\r\n      }, 1000)\r\n    } else if (timeRemaining === 0) {\r\n      endGame()\r\n    }\r\n  }, [timeRemaining, isTimeRunning])\r\n\r\n  return {text, handleChange ,timeRemaining, isTimeRunning, wordCount, textBoxRef, startGame}\r\n\r\n}\r\n\r\nexport default useWordGame","C:\\Users\\ndzzl\\OneDrive\\Desktop\\Projects\\speed-typing-game-react\\src\\pages\\Contact.js",[],"C:\\Users\\ndzzl\\OneDrive\\Desktop\\Projects\\speed-typing-game-react\\src\\pages\\About.js",[],"C:\\Users\\ndzzl\\OneDrive\\Desktop\\Projects\\speed-typing-game-react\\src\\pages\\Game.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"messag